{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20.11.0 || 20.x"
  },
  "scripts": {
    "dev": "powershell -NoProfile -Command \"$env:NEXT_DISABLE_TURBOPACK='1'; $env:NEXT_USE_TURBOPACK='0'; $env:TURBOPACK='0'; $env:WATCHPACK_POLLING='true'; $env:WATCHPACK_POLLING_INTERVAL='1000'; $env:CHOKIDAR_USEPOLLING='true'; $env:CHOKIDAR_INTERVAL='1000'; node .\\node_modules\\next\\dist\\bin\\next dev\"",
    "dev:ps": "powershell -NoProfile -Command \"$env:WATCHPACK_POLLING='true'; $env:WATCHPACK_POLLING_INTERVAL='1000'; $env:CHOKIDAR_USEPOLLING='true'; $env:CHOKIDAR_INTERVAL='1000'; node .\\node_modules\\next\\dist\\bin\\next dev --no-turbo\"",
    "clean": "powershell -NoProfile -Command \"if (Test-Path .next) { Remove-Item -Recurse -Force .next }\"",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "eslint"
  },
  "dependencies": {
    "js-cookie": "^3.0.5",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "lightningcss": "^1.30.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
